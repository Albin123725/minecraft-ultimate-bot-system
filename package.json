{
  "name": "minecraft-ultimate-bot-system",
  "version": "7.0.0",
  "description": "Ultimate Minecraft bot system with 100+ proxies, 10+ accounts, neural AI, and anti-detection",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node setup.js",
    "proxy-test": "node proxy-manager.js",
    "account-test": "node account-manager.js",
    "bot-test": "node bot.js --test",
    "monitor": "node monitoring.js",
    "clean": "rm -rf logs/* sessions/*",
    "full-reset": "npm run clean && npm install && npm start",
    "backup": "cp -r config/ backup/config-$(date +%Y%m%d) && cp -r logs/ backup/logs-$(date +%Y%m%d)",
    "restore": "cp -r backup/config-20240101/* config/ && cp -r backup/logs-20240101/* logs/",
    "gc": "node -e 'if (global.gc) { global.gc(); console.log(\"GC executed\") }'"
  },
  "keywords": [
    "minecraft",
    "bot",
    "proxy",
    "ai",
    "neural",
    "anti-detection",
    "aternos"
  ],
  "author": "Ultimate Bot System",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "ws": "^8.16.0",
    "minecraft-data": "^3.101.0",
    "mineflayer": "^4.25.0",
    "mineflayer-pathfinder": "^2.4.5",
    "prismarine-item": "^1.17.0",
    "vec3": "^0.1.10",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "node-cron": "^3.0.2",
    "user-agents": "^1.0.1100",
    "gc-stats": "^1.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
